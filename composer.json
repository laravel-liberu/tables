{
    "name": "laravel-liberu/tables",
    "description": "Data Table library with server-side processing and a VueJS component",
    "keywords": [
        "laravel-liberu",
        "datatable",
        "data-table",
        "data-table-server-side"
    ],

    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",

            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "box/spout": "^3.0",
        "laravel/framework": "^9.0|^10.0",
        "laravel-liberu/enums": "^2.0",
        "laravel-liberu/filters": "^2.0",
        "laravel-liberu/helpers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Tables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelLiberu\\Tables\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Tables\\AppServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
